      "        &lt;editor *ngSwitchCase=&quot;'html'&quot; [id]=&quot;formItemConf.id&quot; [init]=&quot;{\n",
      "              height: 300,\n",
      "              resize: true,\n",
      "              convert_urls: false,\n",
